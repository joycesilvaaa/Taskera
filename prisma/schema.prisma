// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Tarefa {
  id         Int     @id @default(autoincrement())
  titulo     String
  descricao  String?
  concluida  Boolean @default(false)
}
